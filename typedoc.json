{
    "entryPoints": [
        "apps/api",
        "apps/web"
    ],
    "exclude": ["**/*.spec.ts", "node_modules"],
    "entryPointStrategy": "expand",
    "out": "docs",
    "includeVersion": true,
    "excludeExternals": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "plugin": ["typedoc-plugin-markdown"],
    "jsx": "react"
}